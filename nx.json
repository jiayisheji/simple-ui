{
  "npmScope": "simple-ui",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "simple-ui-doc": {
      "tags": ["doc"]
    },
    "simple-ui-doc-e2e": {
      "tags": [],
      "implicitDependencies": ["simple-ui-doc"]
    },
    "simple-ui": {
      "tags": ["simple-ui"]
    }
  }
}
